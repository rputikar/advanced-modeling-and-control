@article{salar2023fsti,
	title        = {Ifs and Buts of Non-Thermal Processing Technologies for Plant-Based Drinks’ Bioactive Compounds},
	author       = {Salar, Francisco J. and Domínguez-Perles, Raúl and García-Viguera, Cristina and Fernández, Pablo S.},
	volume       = 29,
