@article{ghosh2024asce,
  title         = {Multiscale {{Equation-Oriented Optimization Decreases}} the {{Carbon Intensity}} of {{Shale Gas}} to {{Liquid Fuel Processes}}},
  author        = {Ghosh, Kanishka and Salas, Santiago D. and Garciadiego, Alejandro and Dunn, Jennifer B. and Dowling, Alexander W.},
  publisher     = {American Chemical Society},
  volume        = {12},
