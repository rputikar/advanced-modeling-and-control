@article{kollar2007ijomf,
  title         = {Modeling the Evolution of Droplet Size Distribution in Two-Phase Flows},
  author        = {Koll\'{a}r, L\'{a}szl\'{o} E. and Farzaneh, Masoud},
  volume        = {33},
  number        = {11},
  pages         = {1255--1270},
  doi           = {10.1016/j.ijmultiphaseflow.2007.04.002},
  issn          = {03019322},
  url           = {https://linkinghub.elsevier.com/retrieve/pii/S0301932207000808},
  urldate       = {2022-10-17},
  date          = {2007-11},
  journaltitle  = {International Journal of Multiphase Flow},
  shortjournal  = {International Journal of Multiphase Flow},
  abstract      = {A theoretical model is developed in the present study to simulate droplet motion and the evolution of droplet size distribution (DSD) in two-phase air/dispersed water spray flows. The model takes into account several processes which influence DSD and droplet trajectory: droplet collision and coalescence, evaporation and cooling, gravitational settling, and turbulent dispersion of dispersed phase. The DSDs determined by the model at different locations in a two-phase flow are evaluated by comparing them to experimental observations obtained in an icing wind tunnel. The satisfactory coincidence between simulation and experimental results proves that the model is reliable when modeling two-phase flows under icing conditions. The model is applied for two particular examples in which the modification of DSD is calculated in twophase flows under conditions describing in-cloud icing and freezing drizzle.},
  langid        = {english},
}
@article{kong2019aj,
  title         = {From {{Graphical}} to {{Model}}-based {{Distillation Column Design}}: A {{McCabe}}-{{Thiele}}-inspired {{Mathematical Programming Approach}}},
  shorttitle    = {From {{Graphical}} to {{Model}}-based {{Distillation Column Design}}},
  author        = {Kong, Lingxun and Maravelias, Christos},
  volume        = {65},
  doi           = {10.1002/aic.16731},
  date          = {2019-07-26},
  journaltitle  = {AIChE Journal},
  shortjournal  = {AIChE Journal},
  abstract      = {We propose a mixed-integer nonlinear programming (MINLP) model for simple and complex distillation column design and optimization. The model is based upon the concepts and equations underpinning the McCabe-Thiele method. Generalizing this method, we introduce material balances at various locations of the column and employ binary variables to determine the optimal number of trays and optimal feed locations. We model the vapor-liquid equilibrium using continuous piecewise linear approximating functions. The model is extended to account for multicomponent mixtures and non-constant-molar overflow. We also discuss how to estimate the minimum number of trays and the minimum reflux ratio. This article is protected by copyright. All rights reserved.},
}
@article{konuma2023nm,
  title         = {A near Dimensionally Invariable High-Capacity Positive Electrode Material},
  author        = {Konuma, Itsuki and Goonetilleke, Damian and Sharma, Neeraj and Miyuki, Takuhiro and Hiroi, Satoshi and Ohara, Koji and Yamakawa, Yukio and Morino, Yusuke and Rajendra, Hongahally Basappa and Ishigaki, Toru and Yabuuchi, Naoaki},
  publisher     = {Nature Publishing Group},
  volume        = {22},
  number        = {2},
  pages         = {225--234},
  doi           = {10.1038/s41563-022-01421-z},
  issn          = {1476-4660},
  url           = {https://www.nature.com/articles/s41563-022-01421-z},
  urldate       = {2023-08-04},
  date          = {2023-02},
  journaltitle  = {Nature Materials},
  shortjournal  = {Nat. Mater.},
  abstract      = {Delivering inherently stable lithium-ion batteries is a key challenge. Electrochemical lithium insertion and extraction often severely alters the electrode crystal chemistry, and this contributes to degradation with electrochemical cycling. Moreover, electrodes do not act in isolation, and this can be difficult to manage, especially in all-solid-state batteries. Therefore, discovering materials that can reversibly insert and extract large quantities of the charge carrier (Li+), that is, high capacity, with inherent stability during electrochemical cycles is necessary. Here lithium-excess vanadium oxides with a disordered rocksalt structure are examined as high-capacity and long-life positive electrode materials. Nanosized Li8/7Ti2/7V4/7O2 in optimized liquid electrolytes deliver a large reversible capacity of over 300\,mAh\,g-1 with two-electron V3+/V5+ cationic redox, reaching 750\,Wh\,kg-1 versus metallic lithium. Critically, highly reversible Li storage and no capacity fading for 400\,cycles were observed in all-solid-state batteries with a sulfide-based solid electrolyte. Operando synchrotron X-ray diffraction combined with high-precision dilatometry reveals excellent reversibility and a near dimensionally invariable character during electrochemical cycling, which is associated with reversible vanadium migration on lithiation and delithiation. This work demonstrates an example of an electrode/electrolyte couple that produces high-capacity and long-life batteries enabled by multi-electron transition metal redox with a structure that is near invariant during cycling.},
  issue         = {2},
  langid        = {english},
  keywords      = {Batteries,Nanoparticles},
}
