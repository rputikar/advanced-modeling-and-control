@article{geissler2016cej,
  title         = {Hydrogen Production via Methane Pyrolysis in a Liquid Metal Bubble Column Reactor with a Packed Bed},
  author        = {Gei\ss{}ler, T. and Ab\'{a}nades, A. and Heinzel, A. and Mehravaran, K. and M\"{u}ller, G. and Rathnam, R. K. and Rubbia, C. and Salmieri, D. and Stoppel, L. and St\"{u}ckrad, S. and Weisenburger, A. and Wenninger, H. and family=Wetzel, given=Th., given-i={{Th}}},
