@article{wehinger2015ces,
  title         = {Detailed Numerical Simulations of Catalytic Fixed-Bed Reactors: {{Heterogeneous}} Dry Reforming of Methane},
  shorttitle    = {Detailed Numerical Simulations of Catalytic Fixed-Bed Reactors},
  author        = {Wehinger, Gregor D. and Eppinger, Thomas and Kraume, Matthias},
  volume        = {122},
  pages         = {197--209},
  doi           = {10.1016/j.ces.2014.09.007},
  issn          = {0009-2509},
  url           = {https://www.sciencedirect.com/science/article/pii/S0009250914005016},
  urldate       = {2024-03-26},
  date          = {2015-01-27},
  journaltitle  = {Chemical Engineering Science},
  shortjournal  = {Chemical Engineering Science},
  abstract      = {Highly endothermic (or exothermic) heterogeneous catalytic reactions are performed commonly in fixed-bed reactors with small tube-to-particle-diameter ratios N both in industrial and lab-scale applications. For these reactor configurations conventional plug flow models and pseudo-homogeneous kinetic models fail. An adequate modeling can be carried out with full computational fluid dynamics (CFD) in combination with detailed reaction mechanisms. In this study, a full three-dimensional fixed-bed reactor for the catalytic dry reforming of methane (DRM) over rhodium was simulated with a detailed reaction mechanism. The bed consists of 113 spherical solid particles in which thermal conductivity was considered. Two different Reynolds numbers were investigated, i.e., 35 and 700. The simulated DRM fixed-bed reactor demonstrates the strong interaction between chemical kinetics and transport of momentum, heat and mass. The observed velocity, temperature and species fields are characterized by their three-dimensional behavior and interactions highlighting their complexity and discrepancy from lumped model predictions. In addition, the reaction mechanism determines regions with catalyst deactivation by carbon deposition. This study demonstrates the advantages of modeling heterogeneous catalytic fixed-bed reactors with small N fully in three-dimensional in combination with detailed reaction mechanisms. Finally, this modeling approach reduces dependencies on empiricism for the calculation of multiscale reaction devices.},
  keywords      = {Catalysis,CFD,Dry reforming of methane,Fixed-bed reactors,Modeling},
}
@article{wehinger2015cit,
  title         = {Evaluating {{Catalytic Fixed-Bed Reactors}} for {{Dry Reforming}} of {{Methane}} with {{Detailed CFD}}},
  author        = {Wehinger, Gregor D. and Eppinger, Thomas and Kraume, Matthias},
  volume        = {87},
  number        = {6},
  pages         = {734--745},
  doi           = {10.1002/cite.201400153},
  issn          = {1522-2640},
  url           = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cite.201400153},
  urldate       = {2024-03-26},
  date          = {2015},
  journaltitle  = {Chemie Ingenieur Technik},
  abstract      = {A rigorous modeling of catalytic fixed-bed reactors with spherical and non-spherical particles is presented. The spatially resolved randomly distributed particles allow a comprehensive description of the interstitial flow. The detailed fluid dynamics are coupled with a detailed reaction mechanism of the heterogeneous dry reforming of methane (DRM). Three different kinds of particles of typical industrial dimensions are evaluated under characteristic reaction conditions toward conversions and yields of the DRM. It was found that the fixed bed consisting of cylindrical particles shows the highest conversions and yields. On the contrary, the one-hole cylinder packing exhibits a low performance. With this modeling transport phenomena inside fixed beds can be fully addressed and finally, a substantial quantitative comparison between different packings is possible.},
  langid        = {english},
  keywords      = {Catalysis,Computational fluid dynamics,Dry reforming of methane,Fixed-bed reactors,Modeling},
}
