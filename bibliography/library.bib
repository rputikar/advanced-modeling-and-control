@article{rusumdar2009etafs,
  title         = {Hydrodynamics and Energy Consumption Studies in a Three-Phase Liquid Circulating Three-Phase Fluid Bed Contactor},
  author        = {Rusumdar, Ahmad J and Abuthalib, A. and Mohan, Vaka Murali and Srinivasa Kumar, C. and Sujatha, V. and Rajendra Prasad, P.},
