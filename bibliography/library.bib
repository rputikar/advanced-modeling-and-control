@article{knowlton2005pt,
  title         = {Scale-up of Fluidized-Bed Hydrodynamics},
  author        = {Knowlton, T.M. and Karri, S.B.R. and Issangya, A.},
  volume        = {150},
  number        = {2},
  pages         = {72--77},
  doi           = {10.1016/j.powtec.2004.11.036},
  issn          = {00325910},
  url           = {https://linkinghub.elsevier.com/retrieve/pii/S0032591004004863},
  urldate       = {2022-10-17},
  date          = {2005-02},
  journaltitle  = {Powder Technology},
  shortjournal  = {Powder Technology},
  abstract      = {The scale-up of fluidized beds is not an exact science. However, using proven techniques based on experience and mathematical and/or design models can minimize risk and uncertainty when scaling up fluidized beds. Scaling, which maintains that certain dimensionless groups be matched in different sized units for hydrodynamic similarity to be achieved, is different than scale-up, and generally can not be applied to pilot plants used for scale-up. Scaling is typically more useful to be applied to cold model studies that can be used to improve the operation of an existing plant. Deep fluidized beds of Group A materials can cause significant gas bypassing leading to poor gas-solids contacting. Because commercial beds are generally deeper than beds used in pilot plants, care must be taken to ensure that beds that do not exhibit gas bypassing in smaller units, do not have gas bypassing in commercial systems.},
  langid        = {english},
}
