  shorttitle    = {Thermal Pyrolysis Conversion of Methane to Hydrogen ({{H2}})},
  author        = {Chan, Yi Herng and Chan, Zhe Phak and Lock, Serene Sow Mun and Yiin, Chung Loong and Foong, Shin Ying and Wong, Mee Kee and Ishak, Muhammad Anwar and Quek, Ven Chian and Ge, Shengbo and Lam, Su Shiung},
  pages         = {109329},
