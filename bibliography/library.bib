@article{vashishtha2024efce,
  title         = {Crystallisation: {{Solving}} Crystal Nucleation Problem in the Chemical Engineering Classroom Based on the Research Grade Experiments Deployed in Virtual Mode},
  shorttitle    = {Crystallisation},
  author        = {Vashishtha, Mayank and Kakkar, Shubhangi and Ranjbar, Mahmoud and Kumar, K. Vasanth},
