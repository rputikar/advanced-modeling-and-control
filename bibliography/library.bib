@book{liu2018,
	title        = {Design, Simulation and Optimization of Adsorptive and Chromatographic Separations: A Hands-On Approach},
	shorttitle   = {Design, Simulation and Optimization of Adsorptive and Chromatographic Separations},
	author       = {Liu, Y. A. and Wood, Kevin R. and Yu, Yueying},
