@article{wang2008ces,
	title        = {Length Scale Dependence of Effective Inter-Phase Slip Velocity and Heterogeneity in Gas–Solid Suspensions},
	author       = {Wang, Junwu},
	volume       = 63,
	number       = 8,
	pages        = {2294--2298},
	doi          = {10.1016/j.ces.2008.01.021},
	issn         = {00092509},
	url          = {https://linkinghub.elsevier.com/retrieve/pii/S0009250908000377},
	urldate      = {2022-10-19},
