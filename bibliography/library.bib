@article{tsyurupa2002rafp,
  title         = {Hypercrosslinked Polymers: Basic Principle of Preparing the New Class of Polymeric Materials},
  shorttitle    = {Hypercrosslinked Polymers},
  author        = {Tsyurupa, M. P and Davankov, V. A},
  volume        = {53},
  number        = {2},
