@article{almeida2024ces,
  title         = {Effect of High Solids Loading on Gas–Liquid Mass Transfer in an Oscillatory Flow Reactor for Process Intensification},
  author        = {Almeida, F. and Castro, F. and Teixeira, J. A. and Rocha, F. and Ferreira, A.},
  volume        = {300},
  pages         = {120600},
  doi           = {10.1016/j.ces.2024.120600},
  issn          = {0009-2509},
  url           = {https://www.sciencedirect.com/science/article/pii/S000925092400900X},
  urldate       = {2024-08-31},
  date          = {2024-12-05},
  journaltitle  = {Chemical Engineering Science},
  shortjournal  = {Chemical Engineering Science},
  abstract      = {A wide range of chemical processes involves three phases, where common systems have inefficient mixing and mass transfer is limited. This study uses the oscillatory flow reactor with smooth periodic constrictions (OFR-SPC) according to the patent EP3057694 (B1) to evaluate its potential on O2 mass transfer at high solids loading (10–50~\% (v/v)). For that, the effect of solids (EPS, PVC) properties (concentration, density, and size) on the performance of the volumetric liquid-side mass transfer coefficient (kLa), gas holdup (\ensuremath{\epsilon}G) and Sauter mean bubble diameter (d32) using several operational conditions (oscillation amplitude and frequency, superficial gas velocity) was assessed. Results show that oscillatory conditions can influence the solids effect on kLa. At solid load (\geq{} 30~\% (v/v)) under low oscillations, kLa decreased, \ensuremath{\epsilon}G and d32 increased, slug flow regime emerged and the OFR-SPC behaved as fixed bed reactor. Contrary, no significant influence of solid properties on kLa, and \ensuremath{\epsilon}G was observed at higher oscillations, where the reactor behaved as fluidized bed in a homogeneous flow regime. Notably, solids' concentration ({$<$}30~\% (v/v)), size and density revealed a negligible influence on mass transfer for all range of oscillations, contrary to the widespread consensus where kLa tends to decrease in conventional reactors at lower solid's load ({\$<\$}15~\% (v/v)), making the OFR-SPC a valuable resource for catalytic processes. The OFR-SPC demonstrated higher mass transfer rates (\sim{}3 - fold) than conventional multiphase devices with reasonable power consumption (\sim{}100~W/m-3(-\vert{}-)). These are remarkable results, which indicate the OFR-SPC for three-phase industrial processes intensification.},
  keywords      = {Gas-liquid mass transfer,Multiphase reactor,Oscillatory flow reactor provided with smooth periodic constrictions,Slurry reactor,Solid's concentration},
}
