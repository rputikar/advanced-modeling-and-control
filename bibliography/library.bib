@article{matsusaka2006ces,
	title        = {Simultaneous Measurement of Mass Flow Rate and Charge-to-Mass Ratio of Particles in Gas–Solids Pipe Flow},
	author       = {Matsusaka, S. and Masuda, H.},
	volume       = 61,
	number       = 7,
	pages        = {2254--2261},
	doi          = {10.1016/j.ces.2005.05.006},
