	urldate      = {2022-10-17},
	date         = {2004-04},
	journaltitle = {Powder Technology},
	shortjournal = {Powder Technology},
	abstract     = {Flow structures were determined in a circulating fluidized bed (CFB) riser (0.203 m i.d.Â5.9 m high) of FCC particles (dp=70 Am, qs=1700 kg/m3). A momentum probe was used to measure radial momentum flux profiles at several levels and to distinguish between upward and downward flow regions. Time – mean dynamic pressure (DPm) decreases towards the wall in the range Ug=5 – 8 m/s, Gs=10 – 340 kg/m2 s. The thickness of the annular downflow layer based on DPm=0 reaches a maximum with increasing height. The annular downflow layer disappears locally with increasing solids mass flux ( Gs) at a constant gas velocity, with achievement of the dense suspension upflow (DSU) regime. A new correlation is developed to predict the time – mean thickness of solids down-flowing layer based on solids mass flux and momentum flux. It successfully accounts for the variation of the annular layer thickness with height and Gs, and covers a wide Gs range right up to near the onset of the DSU regime.},
	langid       = {english},
}
@article{kim2007icihamt,
	title        = {Theoretical Approach for a Pressure Drop in Two-Phase Particle-Laden Flows},
	author       = {Kim, Seyun and Lee, Kye Bock and Lee, Chung Gu and Kim, Seong-O},
	volume       = 34,
	number       = 2,
	pages        = {153--161},
	doi          = {10.1016/j.icheatmasstransfer.2006.11.002},
	issn         = {07351933},
	url          = {https://linkinghub.elsevier.com/retrieve/pii/S073519330600203X},
	urldate      = {2022-10-19},
	date         = {2007-02},
	journaltitle = {International Communications in Heat and Mass Transfer},
	shortjournal = {International Communications in Heat and Mass Transfer},
	abstract     = {The purpose of this research is to develop an analytical model for a pressure drop per unit pipe length due to the turbulence modulations of a carrier phase which results from the presence of a dispersed phase in various types of diluted two-phase flows. The wake behind a particle, a particle size, the loading ratio and the density difference between two phases of a particle-laden flow were considered as significant parameters, which have an influence on the turbulence of a particle-laden flow, and the relative velocity of the laden particles was calculated by using a terminal velocity. The frictional pressure drop was formulated by using the force balance in the control volume by considering the shear stresses due to the presence of particles and an analogy of the shear stresses in the solid surfaces. The numerical results show a good agreement with the available experimental data and the model successfully predicted the mechanism of the pressure drop in the particle-laden flows.},
