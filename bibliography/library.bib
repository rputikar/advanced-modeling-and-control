@article{dominguez2021efce,
	title        = {Teaching Chemical Engineering Using Jupyter Notebook: Problem Generators and Lecturing Tools},
	shorttitle   = {Teaching Chemical Engineering Using Jupyter Notebook},
	author       = {Domínguez, J.C. and Alonso, M.V. and González, E.J. and Guijarro, M.I. and Miranda, R. and Oliet, M. and Rigual, V. and Toledo, J.M. and Villar-Chavero, M.M. and Yustos, P.},
