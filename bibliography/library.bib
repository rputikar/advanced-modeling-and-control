@article{th.2022cerad,
  title         = {The Importance/Role of Education in Chemical Engineering},
  author        = {Th., Meyer and Schaer, E. and Abildskov, J. and Feise, H. and Glassey, J. and Liauw, M. and \'{O}'S\'{u}illeabh\'{a}in, C. and Wilk, M.},
