@article{dacunha2023cerad,
	title        = {Design, Conception, and Assessment of an Innovative Liquid Distributor for Separation Column},
	author       = {family=Cunha, given=Sergio, prefix=da, useprefix=true and Dejean, Baptiste and Mizzi, Benoit and Shcherbakova, Nataliya and Rouzineau, David and Meyer, Michel and Gerbaud, Vincent},
	volume       = 196,
	pages        = {377--389},
