@article{taskin2006c&ce,
  title         = {{{FUZZYFCC}}: {{Fuzzy}} Logic Control of a Fluid Catalytic Cracking Unit ({{FCCU}}) to Improve Dynamic Performance},
  shorttitle    = {{{FUZZYFCC}}},
  author        = {Ta\c{s}kin, Harun and Kubat, Cemalettin and Uygun, \"{O}zer and Arslankaya, Seher},
  volume        = {30},
  number        = {5},
  pages         = {850--863},
  doi           = {10.1016/j.compchemeng.2005.12.016},
