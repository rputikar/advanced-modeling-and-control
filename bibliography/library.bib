  author        = {Lin, Yang and Liu, Changshuai and Wang, Lixia and Xu, Xiaozhong and Xu, Shixiang and Shao, Ping},
  volume        = {161},
  pages         = {110864},
  doi           = {10.1016/j.foodhyd.2024.110864},
