@article{keller2022jafc,
  title         = {Structural {{Characterization}} of {{Dietary Fiber}} from {{Different Lupin Species}} ({{Lupinus}} Sp.)},
  author        = {Keller, Judith and Marmit, Sven Peko and Bunzel, Mirko},
  volume        = {70},
