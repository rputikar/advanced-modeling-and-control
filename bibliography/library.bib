@article{siriboonluckul2007icihamt,
  title         = {Turbulence Modelling for Wall-Bounded Particle-Laden Flow with Separation},
  author        = {Siriboonluckul, N. and Juntasaro, V.},
  volume        = {34},
  number        = {3},
  pages         = {331--338},
  doi           = {10.1016/j.icheatmasstransfer.2006.12.003},
  issn          = {07351933},
  url           = {https://linkinghub.elsevier.com/retrieve/pii/S0735193306002314},
  urldate       = {2022-10-19},
  date          = {2007-03},
  journaltitle  = {International Communications in Heat and Mass Transfer},
  shortjournal  = {International Communications in Heat and Mass Transfer},
  abstract      = {The performance of different turbulence models and source-term models for the turbulent separated particle-laden flow is investigated. The accuracy of various turbulence models is firstly evaluated without any modification by the source-term model. The considered models are the k–\ensuremath{\omega} SST model; the standard, the RNG and the realizable k–\ensuremath{\epsilon} models with the standard, the nonequilibrium and the enhanced wall functions. The accuracy of various source-term models is then compared. The results are investigated and analyzed to find the best combination of the turbulence model and the source-term model in predicting the combined effects of turbulence, particles, viscous wall and flow separation. It is found that the RNG k–\ensuremath{\epsilon} turbulence model with the nonequilibrium wall function using the source-term model of Tu and Fletcher is the most suitable combination for this type of flow.},
  langid        = {english},
}
