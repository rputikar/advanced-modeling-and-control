  langid        = {english},
  keywords      = {calcination,clay raw materials,fluidization,thermal enrichment}
}
@article{kadar2024ijohe,
  title         = {Boosting Power Density of Hydrogen Release from {{LOHC}} Systems by an Inverted Fixed-Bed Reactor Design},
  author        = {Kadar, J. and Gackstatter, F. and Ortner, F. and Wagner, L. and Willer, M. and Preuster, P. and Wasserscheid, P. and Gei\ss{}elbrecht, M.},
  volume        = {59},
