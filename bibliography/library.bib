  author        = {Roy, Shantanu and Dudukovi\'{c}, M.P. and Mills, Patrick L.},
  volume        = {61},
  number        = {1--4},
  pages         = {73--85},
  doi           = {10.1016/S0920-5861(00)00352-7},
