@article{biggs2008gm,
	title        = {Granular Temperature in a Gas Fluidized Bed},
	author       = {Biggs, Mark J. and Glass, Don and Xie, L. and Zivkovic, Vladimir and Buts, Alex and Curt Kounders, M. A.},
	volume       = 10,
