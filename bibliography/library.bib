	author       = {Elsheikh, Ammar H. and Sharshir, Swellam W. and Abd Elaziz, Mohamed and Kabeel, A. E. and Guilan, Wang and Haiou, Zhang},
	volume       = 180,
	pages        = {622--639},
	doi          = {10.1016/j.solener.2019.01.037},
