@article{panneerselvam2009ces,
  title         = {{{CFD}} Simulation of Hydrodynamics of Gas–Liquid–Solid Fluidised Bed Reactor},
  author        = {Panneerselvam, R. and Savithri, S. and Surender, G.D.},
