  issn          = {1939-019X},
  url           = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cjce.20500},
  urldate       = {2024-09-06},
  date          = {2011},
  journaltitle  = {The Canadian Journal of Chemical Engineering},
  abstract      = {This work presents CFD-based optimisation of the important geometrical parameters of a cavitating venturi. The parameters for optimisation were selected based on the analysis of the steps involved in the cavitation process like cavity inception, cavity growth, and cavity collapse. It was seen that the ratio of the perimeter of the venturi to the cross-sectional area of its constriction quantifies the possible location of the inception of the cavity. The ratio of the throat length to its height (in the case of a slit venturi) controls the maximum size of the cavity and the angle of the divergence section controls the rate of collapse of a cavity. Based on the numerical study, it was concluded that a slit venturi (\ensuremath{\alpha} = 2.7) with the slit length equal to its height (1:1) and a half angle of divergence section of 5.5\textdegree{} is an optimum geometry for best cavitational activity.},
  langid        = {english},
  keywords      = {annular slit,cavitation,CFD,hydrodynamic,slit,venturi},
}
@article{bashir2022jfpp,
