@article{goldschmidt2001ces,
  title         = {Hydrodynamic Modelling of Dense Gas-\#uidised Beds Using the Kinetic Theory of Granular \#ow: E!Ect of Coe\$cient of Restitution on Bed Dynamics},
  author        = {Goldschmidt, M J V and Kuipers, J A M},
  pages         = {8},
  doi           = {10.1016/S0009-2509(00)00262-1},
  date          = {2001},
  journaltitle  = {Chemical Engineering Science},
  abstract      = {A two-dimensional multi-\#uid Eulerian CFD model with closure laws according to the kinetic theory of granular \#ow has been applied to study the in\#uence of the coe\$cient of restitution on the hydrodynamics of dense gas-\#uidised beds. It is demonstrated that hydrodynamics of dense gas-\#uidised beds (i.e. gas bubbles behaviour) strongly depend on the amount of energy dissipated in particle\vphantom\{\}particle encounters. It is concluded that, in order to obtain realistic bed dynamics from fundamental hydrodynamic models, it is of prime importance to correctly take the e!ect of energy dissipation due to non-ideal particle\vphantom\{\}particle encounters into account. 2001 Elsevier Science Ltd. All rights reserved.},
