@article{yi2023ptrsmpes,
	title        = {Recent Developments of Turbulent Emulsions in Taylor–Couette Flow},
	author       = {Yi, Lei and Wang, Cheng and Huisman, Sander G. and Sun, Chao},
	publisher    = {Royal Society},
	volume       = 381,
	number       = 2243,
	pages        = 20220129,
	doi          = {10.1098/rsta.2022.0129},
	url          = {https://royalsocietypublishing.org/doi/full/10.1098/rsta.2022.0129},
	urldate      = {2023-06-26},
	date         = {2023-01-30},
	journaltitle = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
	abstract     = {Emulsions are common in many natural and industrial settings. Recently, much attention has been paid to understanding the dynamics of turbulent emulsions. This paper reviews some recent studies of emulsions in turbulent Taylor–Couette flow, mainly focusing on the statistics of the dispersed phase and the global momentum transport of the system. We first study the size distribution and the breakup mechanism of the dispersed droplets for turbulent emulsions with a low volume-fraction (dilute) of the dispersed phase. For systems with a high volume-fraction (dense) of the dispersed phase, we address the detailed response of the global transport (effective viscosity) of the turbulent emulsion and its connection to the droplet statistics. Finally, we will discuss catastrophic phase inversions, which can happen when the volume-fraction of the dispersed phase exceeds a critical value during dynamic emulsification. We end the manuscript with a summary and an outlook including some open questions for future research. This article is part of the theme issue ‘Taylor–Couette and related flows on the centennial of Taylor’s seminal Philosophical Transactions paper (part 1)’.},
	keywords     = {phase inversion,Taylor–Couette flow,turbulent emulsions},
}
