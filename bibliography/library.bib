@article{yaqub2020fmai,
	title        = {Flow Pattern Identification and Measurement Techniques in Gas-Liquid-Liquid Three-Phase Flow: A Review},
	shorttitle   = {Flow Pattern Identification and Measurement Techniques in Gas-Liquid-Liquid Three-Phase Flow},
	author       = {Yaqub, Muhammad Waqas and Marappagounder, Ramasamy and Rusli, Risza and Reddy Prasad, D.M. and Pendyala, Rajashekhar},
	volume       = 76,
	pages        = 101834,
