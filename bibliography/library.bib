@article{haim2007psat,
  title         = {Numerical {{Model}} for {{Heat Transfer}} in {{Dilute Turbulent Gas-Particle Flows}}},
  author        = {Haim, M. and Weiss, Y. and Kalman, H.},
  volume        = {25},
  number        = {2},
  pages         = {173--196},
  doi           = {10.1080/02726350701257568},
