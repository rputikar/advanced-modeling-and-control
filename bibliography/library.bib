@article{sharma2023cej,
	title        = {Progress and Challenges in Electrochemical Energy Storage Devices: Fabrication, Electrode Material, and Economic Aspects},
	shorttitle   = {Progress and Challenges in Electrochemical Energy Storage Devices},
	author       = {Sharma, Rahul and Kumar, Harish and Kumar, Gaman and Sharma, Saloni and Aneja, Ranjan and Sharma, Ashok K. and Kumar, Ramesh and Kumar, Parvin},
