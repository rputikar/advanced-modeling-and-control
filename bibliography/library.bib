@article{dry1985pt,
  title         = {Fluidised Beds of Fine, Dense Powders: Scale-up and Reactor Modelling},
  shorttitle    = {Fluidised Beds of Fine, Dense Powders},
  author        = {Dry, R. J. and Judd, M. R.},
  volume        = {43},
  number        = {1},
  pages         = {41--53},
  doi           = {10.1016/S0032-5910(85)80078-4},
  issn          = {0032-5910},
  url           = {https://www.sciencedirect.com/science/article/pii/S0032591085800784},
  urldate       = {2022-10-19},
  date          = {1985-07-01},
  journaltitle  = {Powder Technology},
  shortjournal  = {Powder Technology},
  abstract      = {Changes in the quality of gas--solids contact in going from a 0.05 m dia. fluidised bed of fine iron oxide powder to one with a diameter of 0.64 m have been quantified. An inert gas tracer was employed in the dynamic mode to generate residence time characteristics for each system, and model fitting was used to estimate mass transfer from the bubbles to the dense phase. A comprehensive model screening is carried out to identify the most reasonable modelling approach. Four currently popular models are taken from the literature, and a fifth, based on multiple plug flow units in parallel for the bubble phase, is put forward. This model is termed the multiple bubble-track or MBT model, and screening results show that it describes this particular system on both scales of operation with marginally greater precision than the well-known May--Van Deemter [2, 3] model and significantly greater precision than any of the other three models. This suggests that the multiple plug flow concept is sound and mechanistically more correct than the assumption on which the other models are based. Crossflow ratios estimated on the basis of the MBT model point to the existence of a strong scale-up effect for this fine, dense particle system. As expected [20, 21], most of the crossflow activity occurs in the grid region: this holds for both small and larger beds. The upper regions of the large bed appear to be fairly inactive from an interphase mass transfer point of view and the presence or otherwise of vertical coils is shown to have very little effect on crossflow.},
  langid        = {english},
}
@article{dry1989pt,
  title         = {Gas Residence-Time Characteristics in a High-Velocity Circulating Fluidised Bed of {{FCC}} Catalyst},
  author        = {Dry, R.J. and White, C.C.},
  volume        = {58},
  number        = {1},
  pages         = {17--23},
  doi           = {10.1016/0032-5910(89)80003-8},
  issn          = {00325910},
  url           = {https://linkinghub.elsevier.com/retrieve/pii/0032591089800038},
  urldate       = {2022-10-17},
  date          = {1989-05},
  journaltitle  = {Powder Technology},
  shortjournal  = {Powder Technology},
  abstract      = {Gas residence-time distributions have been measured in a 0.09-m ID high-velocity bed of FCC catalyst. A dynamic gas tracer system based on a pair of rapid-response mass spectrometers was employed, and the tracer itself was argon delivered in the form of a pulse. Concentration- time responses were recorded at two elevations, and system transfer characteristics were extracted in the form of variance increase and peak-to-peak velocity. The variance change in the bed was found to decrease with increasing gas velocity and to increase initially with increasing solids circulation. This increase with a rise in solids flux appeared to weaken at high solids mtes. Peak-to-peak velocities were found to display a maximum at a superficial gas velocity of around 4 - 5 m/s and, when the variance data are processed in to the form of axial dispersion coefficients, a similar peak emerges. One possible explanation is that a radially symmetrical core/annulus structure exists at low velocities, and that beyond about 5 m/s this structure gives way to a more random lateral distribution of lean and dense regions. A possible distinction between ``fast fluidisation''and ``riser transport'' is suggested on this basis.},
  langid        = {english},
}
@article{dry1992pt,
  title         = {Gas--Solid Contact in a Circulating Fluidized Bed: {{The}} Effect of Particle Size},
  shorttitle    = {Gas--Solid Contact in a Circulating Fluidized Bed},
  author        = {Dry, R.J. and White, C.C.},
  volume        = {70},
  number        = {3},
  pages         = {277--284},
  doi           = {10.1016/0032-5910(92)80063-3},
  issn          = {00325910},
  url           = {https://linkinghub.elsevier.com/retrieve/pii/0032591092800633},
  urldate       = {2022-10-19},
  date          = {1992-06},
  journaltitle  = {Powder Technology},
  shortjournal  = {Powder Technology},
  abstract      = {A recently developed technique for measuring the efficiency of contact between gas and solid phases has been used to investigate the fundamental relationship between particle size and contact efficiency in a circulating fluidized bed. Contact efIiciencies have been measured for two grades of sand (140 and 275 pm) and 71 pm FCC catalyst. The measurement technique is based on hot gas as a reacting tracer, with detection by a shrouded rapid-response thermocouple. The rate at which hot gas is thermally equilibrated with the bed is measured and this is interpreted in terms of the degree to which gas and solid come into intimate contact. The results show that, as particles become finer, contact efficiencies improve. However, this improvement is significantly less than the corresponding increase in total gas-solid surface area in the system. Finer particles are likely to form clusters or streamers more readily, and it appears that coarse particles are more surface-efficient in that they expose a greater proportion of external surface to the main gas stream. In other words, the difference in contact efficiency performance between a fine and a coarse powder is reduced by the tendency of the finer powder to aggregate more strongly.},
  langid        = {english},
}
