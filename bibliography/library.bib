@article{rodgers2023efce,
  title         = {An Interactive Graph Resource for Chemical Engineering Teaching},
  author        = {Rodgers, Thomas L. and Bolton, Philip and family=Gryp, given=Percy, prefix=van der, useprefix=true},
  volume        = {45},
  pages         = {80--89},
  doi           = {10.1016/j.ece.2023.08.004},
