@article{gadalla2003cerad,
  title         = {Shortcut {{Models}} for {{Retrofit Design}} of {{Distillation Columns}}},
  author        = {Gadalla, M. and Jobson, M. and Smith, R.},
  series        = {Particle {{Technology}}},
  volume        = {81},
  number        = {8},
  pages         = {971--986},
  doi           = {10.1205/026387603322482211},
