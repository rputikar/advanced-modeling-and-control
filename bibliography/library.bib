@article{asif2024iecr,
  title         = {Australia's {{Spodumene}}: {{Advances}} in {{Lithium Extraction Technologies}}, {{Decarbonization}}, and {{Circular Economy}}},
  shorttitle    = {Australia's {{Spodumene}}},
  author        = {Asif, Abdul Hannan and Li, Chao and Lim, Hazel and Sun, Hongqi},
  publisher     = {American Chemical Society},
  volume        = {63},
  number        = {5},
  pages         = {2073--2086},
