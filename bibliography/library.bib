@article{arguello2023joes,
  title         = {Three-Dimensional Experimental-Scale Phase-Field Modeling of Dendrite Formation in Rechargeable Lithium-Metal Batteries},
  author        = {Arguello, Marcos E and Labanda, Nicol\'{a}s A and Calo, Victor M and Gumulya, Monica and Utikar, Ranjeet and Derksen, Jos},
