@article{wolff2018efce,
  title         = {Shifting Disciplinary Perspectives and Perceptions of Chemical Engineering Work in the 21st Century},
  author        = {Wolff, Karin Elizabeth and Dorfling, Christie and Akdogan, Guven},
  volume        = {24},
  pages         = {43--51},
  doi           = {10.1016/j.ece.2018.06.005},
  issn          = {1749-7728},
