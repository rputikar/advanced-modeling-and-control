@article{banholzer1987iecr,
  title         = {Direct Imaging of Time-Averaged Flow Patterns in a Fluidized Reactor Using x-Ray Computed Tomography},
  author        = {Banholzer, William F. and Spiro, Clifford L. and Kosky, Philip G. and Maylotte, Donald H.},
  volume        = {26},
  number        = {4},
  pages         = {763--767},
  doi           = {10.1021/ie00064a025},
  issn          = {0888-5885, 1520-5045},
  url           = {https://pubs.acs.org/doi/abs/10.1021/ie00064a025},
  urldate       = {2022-10-19},
  date          = {1987-04},
  journaltitle  = {Industrial \& Engineering Chemistry Research},
  shortjournal  = {Ind. Eng. Chem. Res.},
  abstract      = {This paper demonstrates the use of X-ray computed tomography as a noninvasive, nondestructive technique for direct observation of flow patterns averaged over 9 s inside a fluidized bed reactor. The technique allows densities to be obtained with an accuracy of 30 kg/m3 a t a spacial resolution less than 1.5 mm. This information combined with the density of the material fluidized allows direct determination of void fraction a t any location in the bed. The technique could be useful in the verification of fluid dynamics models as well as testing reactor designs. The CT technique has shown the presence of bypass regions which are generally undesirable since they decrease gas-solid contact. CT scanning of model reactors could be a very useful technique to obtain experimental verification of the flow patterns in reactors of different design as well as determining the effectiveness of baffles in eliminating bypassing. A second benefit of the CT technique is the ability to take vertical cross sections a t different locations, i.e., at a diameter and then along several cords parallel to the diameter. This procedure would allow examination of increasing wall effects.},
  langid        = {english},
}
@article{bansal2023mcap,
  title         = {Surface Protection of Wood Using Cerium Oxide Nanoparticles Dispersed Paraffin Wax Nanoemulsion},
  author        = {Bansal, Richa and Pandey, Krishna K.},
  volume        = {306},
  pages         = {128042},
  doi           = {10.1016/j.matchemphys.2023.128042},
  issn          = {0254-0584},
  url           = {https://www.sciencedirect.com/science/article/pii/S0254058423007502},
  urldate       = {2023-08-03},
  date          = {2023-09-15},
  journaltitle  = {Materials Chemistry and Physics},
  shortjournal  = {Materials Chemistry and Physics},
  abstract      = {Exposure to solar UV radiation and other environmental factors promotes the degradation and reduces the serviceability of the wood material under outdoor use. Oils and waxes have been found to be promising solutions for wood protection. This study elucidates the use of paraffin wax based emulsions incorporated with cerium oxide nanoparticles to improve weathering performance of wood. For this purpose, paraffin wax nanoemulsions were prepared by ultrasonication method using tween 80 as surfactant. Cerium oxide nanoparticles (conc. 0.5- to 2\%) were added to provide protection against degradation caused by solar UV radiation. The storage stability of nano emulsion was assessed over a period of 30 days by measuring effective particle size and zeta potential. The microstructure of nanoemulsions was observed by optical microscopy and distribution of CeO2 nanoparticles on coated wood was observed through scanning electron microscopy (SEM). The chemical changes were analyzed by Fourier Transform Infrared spectroscopy (FTIR). Wood specimens of Pinus radiata coated with nanoemulsions were subjected to the accelerated weathering tests for 500h. The time dependent changes in surface color of uncoated and coated wood samples were examined. Moreover, the chemical changes on exposed wood surfaces were analyzed by ATR-FTIR spectroscopy. Stable and homogenous nanoemulsions with particle diameter between 200 and 400~nm were formed and exhibited excellent storage stability. Results of color changes showed that maximum degradation was observed in uncoated wood whereas wood coated with paraffin wax nanoemulsion alone could not improve the UV resistance. However, the addition of cerium oxide nanoparticles proved to be efficient in controlling the color changes on wood surfaces. The degradation of wood components reduced significantly in cerium oxide coated wood which was confirmed by FTIR results.},
  langid        = {english},
  keywords      = {Cerium oxide nanoparticles,Nanoemulsions,Paraffin wax,UV resistance,Weathering,wood},
}
