@article{schouten1994prea,
  title         = {Estimation of the Dimension of a Noisy Attractor},
  author        = {Schouten, Jaap C. and Takens, Floris and family=Bleek, given=Cor M., prefix=van den, useprefix=true},
  publisher     = {American Physical Society},
  volume        = {50},
  number        = {3},
  pages         = {1851--1861},
  doi           = {10.1103/PhysRevE.50.1851},
  url           = {https://link.aps.org/doi/10.1103/PhysRevE.50.1851},
  urldate       = {2024-03-26},
  date          = {1994-09-01},
  journaltitle  = {Physical Review E},
  shortjournal  = {Phys. Rev. E},
  abstract      = {A simple method is proposed to estimate the correlation dimension of a noisy chaotic attractor. The method is based on the observation that the noise induces a bias in the observed distances of trajectories, which tend to appear farther apart than they are. Under the assumption of noise being strictly bounded in amplitude, this leads to a rescaling of interpoint distances on the attractor. A correlation integral function is obtained that accounts for this effect of noise. The applicability of the method is illustrated with two examples, viz., the Lorenz attractor with additive noise and experimental time series of pressure fluctuation data measured in gas-solid fluidized beds.},
}
@article{schouten1996aj,
