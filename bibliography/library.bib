@article{kim2004ces,
  title         = {Flow Behavior and Regime Transition in a High-Density Circulating Fluidized Bed Riser},
  author        = {Kim, Sung Won and Kirbas, Gorkem and Bi, Hsiaotao and Jim Lim, C. and Grace, John R.},
  volume        = {59},
  number        = {18},
  pages         = {3955--3963},
  doi           = {10.1016/j.ces.2004.06.018},
  issn          = {00092509},
  url           = {https://linkinghub.elsevier.com/retrieve/pii/S0009250904003756},
  urldate       = {2022-10-17},
  date          = {2004-09},
  journaltitle  = {Chemical Engineering Science},
  shortjournal  = {Chemical Engineering Science},
  abstract      = {Flow behavior and flow regime transitions were determined in a circulating fluidized bed riser (0.203 m i.d. \texttimes{} 5.9 m high) of FCC particles (dp = 70 m, p = 1700 kg/m3). A momentum probe was used to measure radial profiles of solids momentum flux at several heights and to distinguish between local net upward and downward flow. In the experimental range covered (Ug = 5.8 m/s; Gs = 22.345 kg/m2 s), the fast fluidization flow regime was observed to coexist with dense suspension upflow (DSU). At a constant gas velocity, net downflow of solids near the wall disappeared towards the bottom of the riser with increasing solids mass flux, with dense suspension upflow achieved where there was no refluxing of solids near the riser wall on a time-average basis. The transition to DSU conditions could be distinguished by means of variations of net solids flow direction at the wall, annulus thickness approaching zero and flattening of the solids holdup versus Gs trend. A new flow regime map is proposed distinguishing the fast fluidization, DSU and dilute pneumatic transport flow regimes. ᭧ 2004 Elsevier Ltd. All rights reserved.},
  langid        = {english},
}
@article{kim2004pt,
  title         = {Flow Structure and Thickness of Annular Downflow Layer in a Circulating Fluidized Bed Riser},
  author        = {Kim, Sung Won and Kirbas, Gorkem and Bi, Hsiaotao and Lim, C.Jim and Grace, John R.},
  volume        = {142},
  number        = {1},
  pages         = {48--58},
  doi           = {10.1016/j.powtec.2004.03.011},
  issn          = {00325910},
  url           = {https://linkinghub.elsevier.com/retrieve/pii/S0032591004001172},
  urldate       = {2022-10-17},
  date          = {2004-04},
  journaltitle  = {Powder Technology},
  shortjournal  = {Powder Technology},
  abstract      = {Flow structures were determined in a circulating fluidized bed (CFB) riser (0.203 m i.d.\^{A}5.9 m high) of FCC particles (dp=70 Am, qs=1700 kg/m3). A momentum probe was used to measure radial momentum flux profiles at several levels and to distinguish between upward and downward flow regions. Time – mean dynamic pressure (DPm) decreases towards the wall in the range Ug=5 – 8 m/s, Gs=10 – 340 kg/m2 s. The thickness of the annular downflow layer based on DPm=0 reaches a maximum with increasing height. The annular downflow layer disappears locally with increasing solids mass flux ( Gs) at a constant gas velocity, with achievement of the dense suspension upflow (DSU) regime. A new correlation is developed to predict the time – mean thickness of solids down-flowing layer based on solids mass flux and momentum flux. It successfully accounts for the variation of the annular layer thickness with height and Gs, and covers a wide Gs range right up to near the onset of the DSU regime.},
  langid        = {english},
}
