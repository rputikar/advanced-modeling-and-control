}
@article{zhou2007ces,
  title         = {Modelling of the Turbulent Gas–Particle Flow Structure in a Two-Dimensional Circulating Fluidized Bed Riser},
  author        = {Zhou, Haosheng and Flamant, Gilles and Gauthier, Daniel},
  volume        = {62},
