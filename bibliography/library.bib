@article{rhodes,
	title        = {Characterising Nori-Uniformities in Gas-Particle Flow in the Riser of a Circulating Fluidized Bed},
	author       = {Rhodes, M J and Laussmann, P},
	pages        = 8,
	abstract     = {A sampling probe operating in a non-isokinetic manner was used to measure variations of local solids mass flux with radial, angular and axial position and time in the 152 mm i.d. riser of a circulating fluidixed bed cold model. Time-averaged solids flux profiles exhibit a typically flattened parabolic shape with a region of downward flow adjacent to the riser wall. These profiles do not generally exhibit axial symmetry and a minimum of six radial profiles measurements are found to be necessary to describe the flow pattern adequately. These angular non-uniformities are reproducible, difficult to eliminate and although their form is probably system dependent, it is thought that such non-uniformities are likely to exist in all such gas-particle flow systems to a greater or lesser extent. Detail of the nature of the flowing suspension near to the riser wall has been delineated by analysis of variations in local solids flux with time measured using an adaptation of the same sampling probe.},
	langid       = {english},
	keywords     = {⛔ No DOI found},
}
@article{rhodes1987pt,
	title        = {A Model for the Circulating Fluidized Bed},
	author       = {Rhodes, M.J. and Geldart, D.},
	volume       = 53,
	number       = 3,
	pages        = {155--162},
	doi          = {10.1016/0032-5910(87)80089-X},
	issn         = {00325910},
	url          = {https://linkinghub.elsevier.com/retrieve/pii/003259108780089X},
	urldate      = {2022-10-19},
	date         = {1987-12},
	journaltitle = {Powder Technology},
	shortjournal = {Powder Technology},
	abstract     = {A model for the hydrodynamics of circulating fluidized beds is proposed. The model combines existing entrainment and bed expansion correlations with a system pressure balance. Starting from a knowledge of the powder .size distribution, particle density, gas velocity, equipment geometry and solids inventory, the variation of voidage within the riser is predicted, together with distribution of solids between the riser and supply hopper (or ‘slow’ bed). Qualitative trends with gas velocity and solid circulation flux show good agreement with experiment, but better quantitative agreement must await the development of an improved correlation for entrainment.},
	langid       = {english},
}
@article{rhodes1990pt,
	title        = {Modelling the Flow Structure of Upward-Flowing Gas-Solids Suspensions},
	author       = {Rhodes, M.J.},
	volume       = 60,
	number       = 1,
	pages        = {27--38},
	doi          = {10.1016/0032-5910(90)80101-4},
	issn         = {00325910},
	url          = {https://linkinghub.elsevier.com/retrieve/pii/0032591090801014},
	urldate      = {2022-10-19},
	date         = {1990-01},
	journaltitle = {Powder Technology},
	shortjournal = {Powder Technology},
	abstract     = {A simple sampling probe is developed and tested and found to be successful in measuring radial solids flux profiles in the riser of a cold model circulating fluidized bed. Radial profiles measured at different axial positions and different conditions of gas velocity and imposed solids flux are presented. These profiles have a parabolic form and exhibit a strong downward solids flow near to the riser wall. A core-annulus flow model is proposed for the refluxing dilute-phase transport regime found typically in the circulating fluidized beds, riser reactors, vertical pneumatic conveying and the freeboards of bubbling fluidized beds. The predictions of the model are tested against the experimental results presented here and compared with the findings of other workers. Agreement between predictions and experiment is encouraging.},
	langid       = {english},
