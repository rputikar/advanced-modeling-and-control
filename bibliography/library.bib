@article{gulia2024cej,
  title         = {Eulerian Multi-Fluid Simulations of Local Liquid Distribution in a Bed Packed with Cylindrical Particles},
  author        = {Gulia, Rohit S. and Saxena, D. and Augier, F. and Haroun, Y. and Buwa, Vivek V.},
  volume        = {500},
  pages         = {157287},
  doi           = {10.1016/j.cej.2024.157287},
  issn          = {1385-8947},
  url           = {https://www.sciencedirect.com/science/article/pii/S1385894724087783},
  urldate       = {2024-12-02},
  date          = {2024-11-15},
  journaltitle  = {Chemical Engineering Journal},
  shortjournal  = {Chemical Engineering Journal},
  abstract      = {Packed bed reactors (PBRs) are extensively used in chemical process industries to perform solid-catalyzed gas–liquid reactions. Their performance is significantly influenced by liquid distribution which is a strong function of particle shape and size. In the present work, we simulated the steady-state local liquid distribution, pressure drop (\ensuremath{\Delta}P/L), and overall liquid holdup (〈\ensuremath{\epsilon}L〉) for the beds packed with spherical and cylindrical particles using a three-dimensional (3D) Eulerian multifluid model and validated the predictions using corresponding high-speed imaging measurements performed in pseudo-2D packed beds. We showed that the Eulerian model with existing closures significantly underpredicts the liquid distribution for the bed randomly packed with cylindrical particles. To address this, we proposed a modification in existing mechanical dispersion force (F\rightarrow{}D) model as a function of average particle orientation angle (\texttheta{}). The modification significantly improved agreement between the predictions and the corresponding measurements. Using the modified model, we showed that the increased magnitude of F\rightarrow{}D in the lateral direction leads to higher lateral liquid spreading for cylindrical particles as compared to that for the spherical particles. Importantly, the modified model also predicted the local liquid distribution and \ensuremath{\Delta}P/L for beds packed manually with cylindrical particles oriented at different \texttheta{} in a satisfactory agreement with the corresponding measurements. On increasing \texttheta{}, the F\rightarrow{}D in lateral direction (i.e., perpendicular to flow) as well as liquid–solid interaction force (F\rightarrow{}LS) in axial direction (i.e., parallel to flow) found to increase significantly. As a result, the lateral liquid spreading as well as \ensuremath{\Delta}P/L decrease substantially on increasing \texttheta{}.},
  keywords      = {Cylindrical particles,Eulerian simulations,Liquid distribution,Packed bed reactors,Particle orientation},
}
