@article{biggs2008gm,
  title         = {Granular Temperature in a Gas Fluidized Bed},
  author        = {Biggs, Mark J. and Glass, Don and Xie, L. and Zivkovic, Vladimir and Buts, Alex and Curt Kounders, M. A.},
  volume        = {10},
  number        = {2},
  pages         = {63--73},
  doi           = {10.1007/s10035-007-0077-8},
  issn          = {1434-5021, 1434-7636},
  url           = {http://link.springer.com/10.1007/s10035-007-0077-8},
  urldate       = {2022-10-19},
  date          = {2008-01},
  journaltitle  = {Granular Matter},
  shortjournal  = {Granular Matter},
  abstract      = {The mean square of particle velocity fluctuations, \ensuremath{\delta}v2, which is directly related to the so-called granular temperature, plays an important role in the flow, mixing, segregation and attrition phenomena of particulate systems and associated theories. It is, therefore, important to be able to measure this quantity. We report here in detail our use of diffusing wave spectroscopy (DWS) to measure the mean square particle velocity fluctuations for a 2D non-circulating gas fluidized bed of hollow glass particles whose mean diameter and effective density are 60 \mathrm{\mu}m and 200 kg/m3, respectively. Mean square particle velocity fluctuations were observed to increase with superficial velocity, Us, beyond the minimum fluidization velocity. Following the uniform fluidization theory of Batchelor (1988), the function f (\ensuremath{\varphi}) in the expression \ensuremath{\delta}v2 = f (\ensuremath{\varphi})Us2 was also determined and shown to increase from zero at a solids loading of \ensuremath{\varphi} \approx{} 0.33 to a maximum at \ensuremath{\varphi} \approx{} 0.4 before decreasing again to zero at \ensuremath{\varphi} \approx{} 0.53. The spatial variation of the mean square particle velocity fluctuations was also determined and shown to be approximately symmetrical about the centreline where it is also maximal, and to increase with height above the distributor.},
  langid        = {english},
}
@article{bilgic2023ijohe,
