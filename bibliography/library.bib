@article{babler2008jfm,
	title        = {Modelling the Breakup of Solid Aggregates in Turbulent Flows},
	author       = {Bäbler, Matthäus U. and Morbidelli, Massimo and Bałdyga, Jerzy},
	volume       = 612,
	pages        = {261--289},
	doi          = {10.1017/S002211200800298X},
	issn         = {0022-1120, 1469-7645},
	url          = {https://www.cambridge.org/core/product/identifier/S002211200800298X/type/journal_article},
	urldate      = {2022-10-19},
	date         = {2008-10-10},
	journaltitle = {Journal of Fluid Mechanics},
	shortjournal = {J. Fluid Mech.},
	abstract     = {The breakup of solid aggregates suspended in a turbulent flow is considered. The aggregates are assumed to be small with respect to the Kolmogorov length scale and the flow is assumed to be homogeneous. Further, it is assumed that breakup is caused by hydrodynamic stresses acting on the aggregates, and breakup is therefore assumed to follow a first-order kinetic where               K                                B                              (               x               ) is the breakup rate function and               x               is the aggregate mass. To model               K                                B                              (               x               ), it is assumed that an aggregate breaks instantaneously when the surrounding flow is violent enough to create a hydrodynamic stress that exceeds a critical value required to break the aggregate. For aggregates smaller than the Kolmogorov length scale the hydrodynamic stress is determined by the viscosity and local energy dissipation rate whose fluctuations are highly intermittent. Hence, the first-order breakup kinetics are governed by the frequency with which the local energy dissipation rate exceeds a critical value (that corresponds to the critical stress). A multifractal model is adopted to describe the statistical properties of the local energy dissipation rate, and a power-law relation is used to relate the critical energy dissipation rate above which breakup occurs to the aggregate mass. The model leads to an expression for               K                                B                              (               x               ) that is zero below a limiting aggregate mass, and diverges for               x               → ∞. When simulating the breakup process, the former leads to an asymptotic mean aggregate size whose scaling with the mean energy dissipation rate differs by one third from the scaling expected in a non-fluctuating flow.},
	langid       = {english},
}
