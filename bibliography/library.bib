@article{andreux2005aj,
  title         = {New Description of Fluidization Regimes},
  author        = {Andreux, R. and Gauthier, T. and Chaouki, J. and Simonin, O.},
  volume        = {51},
  number        = {4},
  pages         = {1125--1130},
  doi           = {10.1002/aic.10380},
  issn          = {0001-1541, 1547-5905},
  url           = {https://aiche.onlinelibrary.wiley.com/doi/10.1002/aic.10380},
  urldate       = {2024-09-09},
  date          = {2005-04},
  journaltitle  = {AIChE Journal},
  shortjournal  = {AIChE Journal},
  langid        = {english},
  keywords      = {bubbling regime,experiments,optical probe,transition,turbulent regime},
  abstract      = {Abstract                            Two pressure probes and an intrusive bioptical probe provide experimental data in the bubbling and turbulent regimes to improve our understanding of the transition from the bubbling regime to the fast fluidization regime. Assessment of pressure, voidage, and bubbling properties allows for a new description of turbulent fluidization hydrodynamics. Appreciable changes in the hydrodynamics appear well below the transition criterion U               c               , determined based on pressure fluctuations: bubbles are fast bubbles below U               c               , in bubble–emulsion equilibrium state at U               c               , and then become slow bubbles for higher fluidizing velocity; the maximum of the total fluidizing gas fraction in the bubble phase is reached below U               c               and never exceeds 75–80\%. The fluidizing velocity at which major modifications in hydrodynamics are observed is lower than U               c               . This critical velocity is detected with pressure drop assessment or frequency analysis of voidage fluctuations. Moreover, it can be theoretically calculated from the two-phase modeling correlations supplemented with a new criterion deduced from our experiments. \textcopyright{} 2005 American Institute of Chemical Engineers AIChE J, 2005}
}
