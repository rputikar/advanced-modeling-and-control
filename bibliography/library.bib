@article{sun2023aj,
  title         = {A Density Functional Theory Insight into the Extraction Mechanism of Lithium Recovery from Alkaline Brine by \ensuremath{\beta}-Diketones},
  author        = {Sun, Qing and Chen, Hang and Yu, Jianguo},
  volume        = {69},
  number        = {7},
  pages         = {e18094},
  doi           = {10.1002/aic.18094},
  issn          = {1547-5905},
  url           = {https://onlinelibrary.wiley.com/doi/abs/10.1002/aic.18094},
  urldate       = {2023-07-03},
  date          = {2023},
  journaltitle  = {AIChE Journal},
  abstract      = {In this study, the mechanism of lithium extraction with \ensuremath{\beta}-diketones was investigated by resolving the reaction mechanism, thermodynamic property, and molecular interaction through density functional theory approach. Before the selective extraction of lithium, the \ensuremath{\beta}-diketones had tautomerism with the product of enol. The enol structure was unsteady in alkaline environment so that Li+ can be selectively extracted by taking the place of hydroxyl hydrogen. Meanwhile, the synergistic effect of TOPO intensified the extraction ability by the generation of strong electrostatic. Based on the calculated Gibbs energy changes, the parameters of extraction efficiency and separation factor were analyzed and they had qualitative consistence with the experimental results. Meanwhile, it was determined that the electron-withdrawing effect of the subsistent group in the \ensuremath{\beta}-diketones had a critical influence on the extraction separation performance. Besides, the theoretical model predicted successfully the transition of extraction complex from hydrophilicity to lipophilicity after the combination with synergistic reagent.},
  langid        = {english},
  keywords      = {extraction mechanism,lithium recovery,nosource,salt lake brine,solvent extraction,\ensuremath{\beta}-diketones}
}
