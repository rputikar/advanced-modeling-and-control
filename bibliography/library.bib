@online{lauko2024,
  title         = {Computational Design of Serine Hydrolases},
  author        = {Lauko, Anna and Pellock, Samuel J. and Anischanka, Ivan and Sumida, Kiera H. and Juergens, David and Ahern, Woody and Shida, Alex and Hunt, Andrew and Kalvet, Indrek and Norn, Christoffer and Humphreys, Ian R. and Jamieson, Cooper and Kang, Alex and Brackenbrough, Evans and Bera, Asim K. and Sankaran, Banumathi and Houk, K. N. and Baker, David},
  pages         = {2024.08.29.610411},
  doi           = {10.1101/2024.08.29.610411},
  url           = {https://www.biorxiv.org/content/10.1101/2024.08.29.610411v1},
  urldate       = {2024-09-06},
  date          = {2024-08-30},
  eprinttype    = {bioRxiv},
  eprintclass   = {New Results},
  abstract      = {Enzymes that proceed through multistep reaction mechanisms often utilize complex, polar active sites positioned with sub-angstrom precision to mediate distinct chemical steps, which makes their de novo construction extremely challenging. We sought to overcome this challenge using the classic catalytic triad and oxyanion hole of serine hydrolases as a model system. We used RFdiffusion1 to generate proteins housing catalytic sites of increasing complexity and varying geometry, and a newly developed ensemble generation method called ChemNet to assess active site geometry and preorganization at each step of the reaction. Experimental characterization revealed novel serine hydrolases that catalyze ester hydrolysis with catalytic efficiencies (kcat/Km) up to 3.8 \texttimes{} 103 M-1 s-1, closely match the design models (C\ensuremath{\alpha} RMSDs {$<$} 1 \AA{}), and have folds distinct from natural serine hydrolases. In silico selection of designs based on active site preorganization across the reaction coordinate considerably increased success rates, enabling identification of new catalysts in screens of as few as 20 designs. Our de novo buildup approach provides insight into the geometric determinants of catalysis that complements what can be obtained from structural and mutational studies of native enzymes (in which catalytic group geometry and active site makeup cannot be so systematically varied), and provides a roadmap for the design of industrially relevant serine hydrolases and, more generally, for designing complex enzymes that catalyze multi-step transformations.},
  langid        = {english},
  pubstate      = {prepublished},
}
