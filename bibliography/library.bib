@article{nicastro1984cesuk,
	title        = {Experimental Verification of Scaling Relationships for Fluidized Bed},
	author       = {Nicastro, M. T. and Glicksman, L. R.},
	volume       = {39:9},
	url          = {https://www.osti.gov/etdeweb/biblio/5783940},
	urldate      = {2022-10-25},
	date         = {1984-01-01T05:00:00Z},
	journaltitle = {Chem. Eng. Sci.; (United Kingdom)},
	abstract     = {To verify the scaling relationships for fluidized beds, an atmospheric fluidized bed combustor and a cold bed constructed to operate at the same values of the governing nondimensional parameters were compared. The amplitude and frequency distribution of pressure fluctuations measured in both beds agree closely when these parameters are appropriately nondimensionalized. Thus, bubble characteristics of the two beds should be similar. Disagreements are shown to exist when all the nondimensional parameters are not kept equal between the hot bed and cold model. Variations in the particle size distribution and sphericity also cause non-similar behavior. When the hot bed material is used in the cold bed, there are significant disagreements between the behavior of the two beds. 8 references, 13 figures, 5 tables.},
