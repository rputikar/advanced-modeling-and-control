@article{shahhoseyni2024ar,
  title         = {A Reduced-Order Hybrid Model for Photobioreactor Performance and Biomass Prediction},
  author        = {Shahhoseyni, Shabnam and Greco, Lara and Sivaram, Abhishek and Mansouri, Seyed Soheil},
  volume        = {84},
  pages         = {103750},
  doi           = {10.1016/j.algal.2024.103750},
  issn          = {2211-9264},
  url           = {https://www.sciencedirect.com/science/article/pii/S221192642400362X},
  urldate       = {2024-11-08},
  date          = {2024-12-01},
  journaltitle  = {Algal Research},
  shortjournal  = {Algal Research},
  abstract      = {This paper introduces a hybrid approach for photobioreactor modeling tailored to microalgae cultivation, combining data-driven and mechanistic concepts to improve modeling efficiency and practicality for industrial scale-up applications. Most growth models for microalgae are nonlinear and require experimental measurement of several parameters. The aim of this work is to develop linear practical models for monitoring purposes. A model based on linear coefficients and polynomial features is proposed, balancing interpretability with non-linear representation focusing on model transparency. To simplify the growth model, Taylor series expansion is applied to the Monod and logistic population models. Two scale-specific models are developed and evaluated, offering practical solutions for monitoring microalgae growth in photobioreactors. Therefore, this reduced order representation allows the biomass growth rate to be dependent directly on the biomass concentration. These models do not require exhaustive data collection of substrate concentration over time, making them cost-effective and efficient for industrial applications. This work provides a step forward in photobioreactor modeling, contributing to the sustainable production of microalgae.},
  keywords      = {Hybrid model,Microalgae,Photobioreactor,Reduced order model},
}
