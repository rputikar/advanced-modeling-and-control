@article{bahr2014l-fsat,
  title         = {Chemical Composition of Dehulled Seeds of Selected Lupin Cultivars in Comparison to Pea and Soya Bean},
  author        = {B\"{a}hr, Melanie and Fechner, Anita and Hasenkopf, Katrin and Mittermaier, Stephanie and Jahreis, Gerhard},
  volume        = {59},
