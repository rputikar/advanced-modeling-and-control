@incollection{zhang2023ctpifrmdemaea,
  title         = {{{DEM Coupled}} with {{Lattice-Boltzmann Method}} ({{LBM}})},
  author        = {Zhang, Fengshou and Damjanac, Branko and Furtney, Jason},
  booktitle     = {Coupled {{Thermo-Hydro-Mechanical Processes}} in {{Fractured Rock Masses}}: {{Discrete Element Modeling}} and {{Engineering Applications}}},
  location      = {Cham},
  publisher     = {Springer International Publishing},
  pages         = {133--159},
  doi           = {10.1007/978-3-031-25787-2\_5},
  isbn          = {978-3-031-25787-2},
  url           = {https://doi.org/10.1007/978-3-031-25787-2\_5},
  urldate       = {2023-08-17},
  editor        = {Zhang, Fengshou and Damjanac, Branko and Furtney, Jason},
  date          = {2023},
  abstract      = {The lattice-Boltzmann method (LBM) is a powerful tool for simulating incompressible viscous flow, offering several advantages over traditional computational fluid dynamics (CFD) approaches. Instead of solving Navier–Stokes equations, the LBM uses a discrete mesoscopic approach that represents fluid as discrete parcels moving in a fixed lattice, subject to simple collision rules. As a result, an LBM solver is simpler and more computationally efficient than a traditional CFD solver. The LBM-DEM coupling method can naturally represent fluid flow in the pores formed by large particles and can accommodate multiphase flow, making it particularly useful for modeling complex fluid-solid interactions. This chapter presents a direct derivation of the lattice Boltzmann equation from the continuous Boltzmann equation, discretized in a special manner in both time and phase space. In addition, this chapter also presents a formulation of the DEM/LBM coupling method, along with a modeling of particle-fluid interaction. Two examples are provided to demonstrate the advantages of the coupling method: pore-scale fluid flow through idealized porous media and modeling of multi-phase fluid in porous rocks.},
  langid        = {english}
}
@article{zhang2023d,
  title         = {Investigation and Application of Microscopic Mechanism of Multiple Bubble Movement on Directional Frozen Crystallization of Seawater},
  author        = {Zhang, Da and Li, Yan and Yuan, Han},
  pages         = {116902},
  doi           = {10.1016/j.desal.2023.116902},
