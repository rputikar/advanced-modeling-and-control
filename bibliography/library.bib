@article{page2024jfm,
  title         = {Exact Coherent Structures in Two-Dimensional Turbulence Identified with Convolutional Autoencoders},
  author        = {Page, Jacob and Holey, Joe and Brenner, Michael P. and Kerswell, Rich R.},
  volume        = {991},
  pages         = {A10},
  doi           = {10.1017/jfm.2024.552},
  issn          = {0022-1120, 1469-7645},
  url           = {https://www.cambridge.org/core/journals/journal-of-fluid-mechanics/article/exact-coherent-structures-in-twodimensional-turbulence-identified-with-convolutional-autoencoders/B54D02CA985CA291CB1FFC83279CF772},
  urldate       = {2024-09-11},
  date          = {2024-07},
  journaltitle  = {Journal of Fluid Mechanics},
  abstract      = {, Convolutional autoencoders are used to deconstruct the changing dynamics of two-dimensional Kolmogorov flow as ReReRe is increased from weakly chaotic flow at Re=40Re=40Re=40 to a chaotic state dominated by a domain-filling vortex pair at Re=400Re=400Re=400. `Latent Fourier analysis' (Page et al., Phys. Rev. Fluids 6, 2021, p. 034402) reveals a detached class of bursting dynamics at Re=40Re=40Re=40 which merge with the low-dissipation dynamics as ReReRe is increased to 100100100 and provides an efficient representation within which to find unstable periodic orbits (UPOs) using recurrent flow analysis. Focusing on initial guesses with energy in higher latent Fourier wavenumbers allows a significant number of high-dissipation-rate UPOs associated with the bursting events to be found for the first time. At Re=400Re=400Re=400, the UPOs discovered at lower ReReRe move away from the attractor, and an entirely different embedding structure is formed within the network devoid of small-scale vortices. Here latent Fourier projections identify an associated `large-scale' UPO which we believe to be a finite-ReReRe continuation of a solution to the Euler equations.},
  langid        = {english},
  keywords      = {low-dimensional models},
}
