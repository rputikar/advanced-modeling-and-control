@article{ghendour2020m,
  title         = {Review of Measurement Techniques for Void Fraction of Two-Phase Flow through Annulus},
  author        = {Ghendour, Nabil and Meribout, Mahmoud and Azzi, Abdelwahid},
  volume        = {165},
  pages         = {108196},
  doi           = {10.1016/j.measurement.2020.108196},
