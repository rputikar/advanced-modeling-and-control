  author        = {Travis, J. R. and Piccioni Koch, D. and Breitung, W.},
  series        = {{{HySafe}} 1},
  volume        = {37},
  number        = {22},
  pages         = {17373--17379},
  doi           = {10.1016/j.ijhydene.2012.07.077},
