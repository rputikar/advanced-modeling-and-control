  author        = {Lao, Liangfeng and Aguirre, Andres and Tran, Anh and Wu, Zhe and Durand, Helen and Christofides, Panagiotis D.},
  volume        = {148},
  pages         = {78--92},
  doi           = {10.1016/j.ces.2016.03.038},
  issn          = {0009-2509},
  url           = {https://www.sciencedirect.com/science/article/pii/S0009250916301531},
  urldate       = {2024-03-26},
